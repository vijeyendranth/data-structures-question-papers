//question 2c
#include <stdio.h>

// 1. Recursive Method
long long fib_recursive(int n) {
    if (n <= 1) {
        return n;
    }
    return fib_recursive(n - 1) + fib_recursive(n - 2);
}

// 2. Iterative Method (More efficient)
long long fib_iterative(int n) {
    if (n <= 1) {
        return n;
    }
    long long a = 0, b = 1, c;
    for (int i = 2; i <= n; i++) {
        c = a + b;
        a = b;
        b = c;
    }
    return b;
}

int main() {
    int n = 10; // Example: Find the 10th Fibonacci number

    printf("Recursive Fibonacci of %d: %lld\n", n, fib_recursive(n));
    printf("Iterative Fibonacci of %d: %lld\n", n, fib_iterative(n));

    // Justification:
    // The iterative method is more efficient. The recursive method has a time
    // complexity of O(2^n) due to redundant calculations, while the iterative
    // method has a linear time complexity of O(n). The recursive method
    // can also lead to stack overflow for large values of 'n'.

    return 0;
}
